#region-placas.container
  .region
    .region-title
      %h1= page_title!('placa')

  .region
    %p
      %b Nombre:
      = @placa.nombre

  .region
    - if current_admin
      .admin-controls
        = link_to 'Editar', edit_placa_path(@placa), class: 'btn btn-success'
    = link_to 'Regresar', placas_path, class: 'btn btn-warning'
